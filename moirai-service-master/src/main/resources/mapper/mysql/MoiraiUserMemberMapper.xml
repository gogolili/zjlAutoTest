    <update id="updateByPrimaryKeySelective" parameterType="com.baiwang.moirai.model.user.MoiraiUserMember">
        update moirai_user_member
        <set>
            <if test="tenantId != null">
                TENANT_ID = #{tenantId,jdbcType=BIGINT},
            </if>
            <if test="orgId != null">
                ORG_ID = #{orgId,jdbcType=BIGINT},
            </if>
            <if test="userId != null">
                USER_ID = #{userId,jdbcType=BIGINT},
            </if>
            <if test="bindUserId != null">
                BIND_USER_ID = #{bindUserId,jdbcType=BIGINT},
            </if>
            <if test="defaultOrgFlag != null">
                DEFAULT_ORG_FLAG = #{defaultOrgFlag,jdbcType=VARCHAR},
            </if>
            <if test="defaultValidationFlag != null">
                DEFAULT_VALIDATION_FLAG = #{defaultValidationFlag,jdbcType=VARCHAR},
            </if>
            <if test="createTime != null">
                CREATE_TIME = #{createTime,jdbcType=TIMESTAMP},
            </if>
            <if test="updateTime != null">
                UPDATE_TIME = #{updateTime,jdbcType=TIMESTAMP},
            </if>
            <if test="orderNum != null">
                ORDER_NUM = #{orderNum,jdbcType=INTEGER},
            </if>
            <if test="delFlag != null">
                DEL_FLAG = #{delFlag,jdbcType=VARCHAR},
            </if>
            <if test="updateUser != null">
                UPDATE_USER = #{updateUser,jdbcType=VARCHAR},
            </if>
        </set>
        where MEMBER_ID = #{memberId,jdbcType=BIGINT}
    </update>

    <update id="updateMoreMember" parameterType="java.util.List">
        <foreach collection="list" item="item" index="index" open="" close="" separator=";">
            UPDATE moirai_user_member
            <set>
                <if test="item.defaultOrgFlag != null">
                    DEFAULT_ORG_FLAG=#{item.defaultOrgFlag},
                </if>
                <if test="item.defaultValidationFlag != null">
                    DEFAULT_VALIDATION_FLAG=#{item.defaultValidationFlag},
                </if>
                <if test="item.updateTime != null">
                    UPDATE_TIME=#{item.updateTime},
                </if>
            </set>
            where member_id = #{item.memberId}
        </foreach>
    </update>

    <select id="selectBindFactorTenant" parameterType="com.baiwang.moirai.model.user.MoiraiUserMember"
            resultMap="BaseResultMap">
        select min(member.TENANT_ID) as tenant_id, min(member.BIND_USER_ID) as bind_user_id
        from moirai_user_member member
        left join moirai_tenant tenant
        on member.TENANT_ID = tenant.tenant_id
        where member.USER_ID = #{userId,jdbcType=BIGINT} and member.DEL_FLAG = 'N'
        and (tenant.dual_factor = '1' or tenant.dual_factor = '2')
        group by member.USER_ID
    </select>
</mapper>