    select
    <include refid="Base_Column_List" />
    from moirai_org_history
    where
    1=1
    <if test="taxCode != null and taxCode !=''"> AND tax_code = #{taxCode} </if>
    <if test="orgName != null and orgName !=''"> AND org_name = #{orgName} </if>
  </select>

  <delete id="deleteByPrimaryKey" parameterType="java.lang.Long">
    delete from moirai_org_history
    where his_id = #{hisId,jdbcType=BIGINT}
  </delete>
  <insert id="insertSelective" parameterType="com.baiwang.moirai.model.org.MoiraiOrgHistory">
