<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baiwang.moirai.mapper.MoiraiOrgHistoryMapper">
  <resultMap id="BaseResultMap" type="com.baiwang.moirai.model.org.MoiraiOrgHistory">
